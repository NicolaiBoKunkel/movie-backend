			"item": [
				{
					"name": "Login - Existing Admin",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"const adminUsername = pm.environment.get('ADMIN_USERNAME') || 'admin_test_user';",
									"pm.environment.set('NEO4J_ADMIN_USERNAME', adminUsername);"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    pm.test(\"Status code is 200\", function () {",
									"        pm.response.to.have.status(200);",
									"    });",
									"    ",
									"    const jsonData = pm.response.json();",
									"    pm.environment.set('NEO4J_ADMIN_TOKEN', jsonData.token);",
									"    ",
									"    pm.test(\"Logged in with admin role\", function () {",
									"        pm.expect(jsonData.user.role).to.equal('admin');",
									"    });",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
