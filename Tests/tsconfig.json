{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "esnext"],
    "types": ["cypress", "node"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": false,
    "allowJs": true,
    "skipLibCheck": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.js",
    "./**/*.cjs",
    "./**/*.mjs"
  ]
}
